<?xml version='1.0' encoding='utf-8'?>
<widget id="sg.edu.sg.cs5231" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>Vulnerable Phonegap</name>
