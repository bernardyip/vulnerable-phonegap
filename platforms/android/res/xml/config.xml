<?xml version='1.0' encoding='utf-8'?>
<widget id="sg.edu.sg.cs5231" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <feature name="Contacts">
        <param name="android-package" value="org.apache.cordova.contacts.ContactManager" />
    </feature>
    <feature name="BarcodeScanner">
        <param name="android-package" value="com.phonegap.plugins.barcodescanner.BarcodeScanner" />
    </feature>
    <feature name="VulnerablePlugin">
        <param name="android-package" value="cordova.plugin.vulnerable.VulnerablePlugin" />
    </feature>
    <feature name="DecryptResource">
        <param name="android-package" value="com.tkyaji.cordova.DecryptResource" />
        <param name="onload" value="true" />
    </feature>
    <name>Vulnerable Phonegap</name>
    <description>
        Phonegap app that is vulnerable to xss
    </description>
    <author email="support@phonegap.com">
        CS5231 Team
    </author>
    <content src="/+++/index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="market:*" />
    <preference name="loglevel" value="DEBUG" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="14" />
    <preference name="android-targetSdkVersion" value="23" />
</widget>
